<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <listener>
        <listener-class>by.iba.student.web.listener.AppListener</listener-class>
    </listener>

    <servlet>
        <servlet-name>StudentServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.StudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentPageServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.StudentPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddStudentServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.AddStudentServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ProfessorsServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.ProfessorsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfessorsPageServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.ProfessorsPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProfessorServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.AddProfessorServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>MarksServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.MarksServelt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkPageServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.MarkPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddMarkServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.AddMarkServelt</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>GroupsServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.GroupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GroupPageServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.GroupPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddGroupServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.AddGroupServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>SubjectsServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.SubjectsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubjectPageServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.SubjectPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddSubjectServlet</servlet-name>
        <servlet-class>by.iba.student.web.servlet.AddSubjectServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StudentServlet</servlet-name>
        <url-pattern>/students</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StudentPageServlet</servlet-name>
        <url-pattern>/studentsPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddStudentServlet</servlet-name>
        <url-pattern>/studentsPage/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ProfessorsServlet</servlet-name>
        <url-pattern>/professors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfessorsPageServlet</servlet-name>
        <url-pattern>/professorsPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProfessorServlet</servlet-name>
        <url-pattern>/professorsPage/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>MarksServlet</servlet-name>
        <url-pattern>/marks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkPageServlet</servlet-name>
        <url-pattern>/marksPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddMarkServlet</servlet-name>
        <url-pattern>/marksPage/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GroupsServlet</servlet-name>
        <url-pattern>/groups</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GroupPageServlet</servlet-name>
        <url-pattern>/groupsPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddGroupServlet</servlet-name>
        <url-pattern>/groupsPage/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>SubjectsServlet</servlet-name>
        <url-pattern>/subjects</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubjectPageServlet</servlet-name>
        <url-pattern>/subjectsPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddSubjectServlet</servlet-name>
        <url-pattern>/subjectsPage/add</url-pattern>
    </servlet-mapping>
